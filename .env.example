# Nuron Memory Plugin - Environment Template
# Copy this file to .env and fill in your values

# OpenRouter API (LLM + Embeddings)
# Get your key from https://openrouter.ai/settings/keys
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_API_URL=https://openrouter.ai/api/v1

# Neo4j Database
# For local docker: bolt://localhost:7687
# For cloud: bolt://your-neo4j-cloud.io:7687
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your-neo4j-password
NEO4J_DATABASE=neo4j

# Graphiti Settings
# Unique identifier for this user's memory (e.g., "default", "main", or your name)
GRAPHITI_GROUP_ID=default
SEMAPHORE_LIMIT=3
